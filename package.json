{
  "name": "eltor-app-monorepo",
  "version": "1.0.0",
  "private": true,
  "description": "Monorepo for eltor-app",
  "scripts": {
    "dev:web": "cd web && pnpm i && pnpm run build && pnpm run dev",
    "dev:electron": "cd electron && pnpm run dev",
    "build:web": "cd web && pnpm run build",
    "build:electron": "cd electron && pnpm run build",
    "install:all": "cd shared/eltor-components && pnpm install && cd ../nextjs && pnpm install && cd ../electron && pnpm install",
    "start:web": "cd web && pnpm run start",
    "start:electron": "cd electron && pnpm start"
  },
  "workspaces": [
    "electron",
    "web"
  ],
  "devDependencies": {
    "concurrently": "^8.0.0"
  },
  "packageManager": "pnpm@9.9.0+sha256.7a4261e50d9a44d9240baf6c9d6e10089dcf0a79d0007f2a26985a6927324177"
}
